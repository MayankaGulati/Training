		HASHES:

.new method


3.0.1 :002 > Hash.new
 => {} 
3.0.1 :003 > aw=Hash.new
 => {} 
3.0.1 :004 > aw
 => {} 
3.0.1 :005 > aw.class
 => Hash 
3.0.1 :006 > aw = Hash[foo: 0,bar: 1,baz: 1]
 => {:foo=>0, :bar=>1, :baz=>1} 
3.0.1 :007 > aw
 => {:foo=>0, :bar=>1, :baz=>1} 


.delete method

3.0.1 :002 > h = Hash[a:10,b:20,c:30]
 => {:a=>10, :b=>20, :c=>30} 
3.0.1 :003 > h.delete(:b)
 => 20 
3.0.1 :004 > h
 => {:a=>10, :c=>30} 

